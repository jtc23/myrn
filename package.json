		"react-native-scrollable-tab-view": "^0.10.0",
		"react-native-sortable-listview": "^0.2.9",
		"react-native-tab-navigator": "^0.3.4"
	},
	"devDependencies": {
		"babel-jest": "24.8.0",
		"babel-preset-react-native": "4.0.1",
		"jest": "24.8.0",
